{
  "environment": "staging",
  "api": {
    "baseUrl": "https://staging.api.example.com",
    "licenseEndpoint": "https://staging.api.example.com/license",
    "analyticsEndpoint": "https://staging.api.example.com/analytics",
    "timeout": 10000
  },
  "features": {
    "enableBetaFeatures": true,
    "enableDebugMode": false,
    "enableTelemetry": true,
    "enableAutoUpdate": false
  },
  "logging": {
    "level": "info",
    "console": true,
    "remote": true
  },
  "rateLimit": {
    "requestsPerMinute": 100,
    "burstSize": 15
  }
}
