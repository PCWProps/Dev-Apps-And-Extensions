{
  "environment": "development",
  "api": {
    "baseUrl": "https://dev.api.example.com",
    "licenseEndpoint": "https://dev.api.example.com/license",
    "analyticsEndpoint": "https://dev.api.example.com/analytics",
    "timeout": 10000
  },
  "features": {
    "enableBetaFeatures": true,
    "enableDebugMode": true,
    "enableTelemetry": true,
    "enableAutoUpdate": false
  },
  "logging": {
    "level": "debug",
    "console": true,
    "remote": false
  },
  "rateLimit": {
    "requestsPerMinute": 120,
    "burstSize": 20
  }
}
